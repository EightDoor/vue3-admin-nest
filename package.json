    "eslint-plugin-import": "^2.26.0",
    "jest": "27.3.1",
    "prettier": "^2.6.2",
    "ts-jest": "27.0.7",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3",
