    "rxjs": "^6.6.7",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.15",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.22",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.41",
