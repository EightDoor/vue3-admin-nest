    "pino-pretty": "^4.7.1",
    "prettier": "^2.2.1",
    "start-server-nestjs-webpack-plugin": "^2.2.5",
    "ts-jest": "26.5.5",
    "ts-loader": "^9.0.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
